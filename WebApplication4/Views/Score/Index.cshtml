@model IEnumerable<WebApplication4.Score>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Strokes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScoreCard.Round.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hole._Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Strokes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ScoreCard.Round.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hole._Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
